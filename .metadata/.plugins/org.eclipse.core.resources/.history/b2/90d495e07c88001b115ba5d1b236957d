package com.bankapp.main.menu;

import org.apache.log4j.Logger;



public class Menu {
	private static Logger Log = Logger.getLogger(Menu.class);

	
	public void printUserMenu() {
		Log.info("User MENU");
		Log.info("------------------");
		Log.info("Are you a employee or customer?");
		Log.info("1) Employee");
		Log.info("2) Customer");
	}

}
